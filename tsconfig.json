{
  "compilerOptions": {
    "lib": [
      "es2016",
      "dom"
    ],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "module": "esnext",
    "target": "es5",
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "@src/*": [
        "src/*"
      ]
    },
  },
  "include": [
    "src",
    "src/images.d.ts"
  ]
}